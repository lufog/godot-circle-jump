config_version=5

[application]

config/name="Circle Jump"
run/main_scene="res://main/main.tscn"
config/features=PackedStringArray("4.2")
config/icon="res://icon.png"
config/tags=PackedStringArray("2d", "kidscancode")

[autoload]

Settings="*res://settings.gd"

[display]

window/size/viewport_width=480
window/size/viewport_height=854
window/stretch/mode="viewport"
window/handheld/orientation=1

[gui]

fonts/dynamic_fonts/use_oversampling=false
_global_script_classes=[{
"base": "CanvasLayer",
"class": &"BaseScreen",
"language": &"GDScript",
"path": "res://ui/base_screen.gd"
}, {
"base": "Area2D",
"class": &"Circle",
"language": &"GDScript",
"path": "res://objects/circle/circle.gd"
}, {
"base": "CanvasLayer",
"class": &"HUD",
"language": &"GDScript",
"path": "res://ui/hud/hud.gd"
}, {
"base": "Area2D",
"class": &"Jumper",
"language": &"GDScript",
"path": "res://objects/jumper/jumper.gd"
}, {
"base": "Node",
"class": &"Screens",
"language": &"GDScript",
"path": "res://ui/screens/screens.gd"
}]
_global_script_class_icons={
"BaseScreen": "",
"Circle": "",
"HUD": "",
"Jumper": "",
"Screens": ""
}

[input_devices]

pointing/emulate_touch_from_mouse=true
